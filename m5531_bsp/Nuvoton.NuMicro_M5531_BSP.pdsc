      <require condition="NuMicro M5531 Device"/>
      <require Cclass="CMSIS Driver" Cgroup="Common" />
      <require Cclass="Device" Cgroup="StdDriver" Csub="SDH"/>
    </condition>

    <condition id="CMSIS Driver SAI">
