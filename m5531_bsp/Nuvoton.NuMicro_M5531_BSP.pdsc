<?xml version="1.0" encoding="utf-8"?>

<package schemaVersion="1.7.44" xmlns:xs="http://www.w3.org/2001/XMLSchema-instance" xs:noNamespaceSchemaLocation="https://raw.githubusercontent.com/Open-CMSIS-Pack/Open-CMSIS-Pack-Spec/v1.7.44/schema/PACK.xsd">
  <!-- Must have - software pack vendor name: -->
  <vendor>Nuvoton</vendor>
  <!-- Must have - software pack name: -->
  <name>NuMicro_M5531_BSP</name>
  <!-- Strongly recommended - pack description with Overview.md file: -->
  <description overview="README.md">Board Support Pack for M5531 Series</description>
  <!-- Optional - pack download URL (if publicly available): -->
  <url>https://github.com/OpenNuvoton/keil_packs/raw/master/m5531_bsp/</url>
  <!-- Optional - Git repo URL (if applicable): -->
  <repository type="git">https://github.com/OpenNuvoton/M5531BSP.git</repository>
  <!-- Strongly recommended - support contact information: -->
  <supportContact></supportContact>
   <!-- Must have - licensing information (for software BOM): -->
  <license>LICENSE</license>
  <licenseSets>
    <licenseSet id="all" default="true" gating="true">
      <license name="LICENSE" title="Apache 2.0 License"/>
    </licenseSet>
  </licenseSets>
 
  <!-- Must have - pack release information for versioning: -->
  <releases>
    <release version="3.01.002" date="2025-08-08">
      Supported M5531B.
      Added CMSIS drivers, new sample codes.
      Updated startup, standard drivers, libraries, sample codes.
      Removed RTC_TimeAndTick sample code.
    </release>
  </releases>

  <keywords>
    <!-- Optional - keywords for indexing: -->
    <keyword>Nuvoton</keyword>
    <keyword>Board Support Pack</keyword>
    <keyword>NuMicro_M5531</keyword>
    <keyword>M5531 BSP</keyword>
    <keyword>M5531H2LJAE</keyword>
    <keyword>NuMaker-M5531</keyword>
  </keywords>

  <requirements>
    <!-- Strongly recommended - requirement to DFP (or other packs): -->
    <packages>
      <package vendor="Nuvoton" name="NuMicroM55_DFP" version="1.0.2"/>
      <package vendor="ARM" name="CMSIS" version="6.1.0"/>
    </packages>
  </requirements>

  <!-- Must have - boards section: -->
  <boards>
    <!-- Must have - board information: -->
    <board vendor="Nuvoton" name="NuMaker-M5531" revision="V1" salesContact="sales@nuvoton.com" orderForm="https://direct.nuvoton.com/en/numaker-x-M5531d">
      <!-- Strongly recommended - board description: -->
      <description>NuMaker-X-M5531D Board</description>
      <!-- Must have - on-board device information: -->
      <mountedDevice    deviceIndex="0" Dvendor="Nuvoton:18" Dname="M5531H2LJAE"/>
      <!-- Strongly recommended - on-board device compatibility information: -->
      <compatibleDevice deviceIndex="0" Dvendor="Nuvoton:18" Dname="M5531*"/>
      <!-- Strongly recommended - board features: -->
      <feature type="ODbg"      n="1"                name="On-board Nu-Link2-Me"/>
      <feature type="PWR"       n="5"                name="USB Powered"/>
      <feature type="PWR"       n="3.3" m="5"        name="POWER Connector for external supply"/>
      <feature type="DIO"       n="176"              name="4 extended connectors to all chip pins"/>
      <feature type="LED"       n="3"                name="User LED"/>
      <feature type="Button"    n="2"                name="User Button"/>
      <feature type="USB"       n="2"                name="USB OTG Host/Device Connector"/>
      <!-- Strongly recommended - on-board debug probe information (if applicable): -->
      <debugInterface adapter="Nu-Link" connector="USB-C"/>
    </board>
  </boards>
  
  <!-- Must have - conditions section: -->
  <conditions>
    <!-- Must have - condition related to the device family: -->
    <condition id="NuMicro M5531 Device">
      <description>NuMicro M5531 Devcies</description>
      <require Dvendor="Nuvoton:18" Dfamily="NuMicro M55 Family" DsubFamily="M5531" Dname="M5531*"/>
    </condition>

    <condition id="Toolchain ARMCC">
      <accept Tcompiler="ARMCC"/> 
    </condition>

    <condition id="Toolchain GCC">
      <accept Tcompiler="GCC"/> 
    </condition>

    <condition id="Toolchain IAR">
      <accept Tcompiler="IAR"/> 
    </condition>

    <condition id="PowerDeliveryLib Driver UTCPD">
      <description>PowerDeliveryLib UTCPD Driver</description>
      <require condition="NuMicro M5531 Device"/>
      <require Cclass="Device" Cgroup="StdDriver" Csub="UTCPD"/>
