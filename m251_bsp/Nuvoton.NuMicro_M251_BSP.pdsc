<?xml version="1.0" encoding="utf-8"?>

<package schemaVersion="1.7.44" xmlns:xs="http://www.w3.org/2001/XMLSchema-instance" xs:noNamespaceSchemaLocation="https://raw.githubusercontent.com/Open-CMSIS-Pack/Open-CMSIS-Pack-Spec/v1.7.44/schema/PACK.xsd">
  <!-- Must have - software pack vendor name: -->
  <vendor>Nuvoton</vendor>
  <!-- Must have - software pack name: -->
  <name>NuMicro_M251_BSP</name>
  <!-- Strongly recommended - pack description with Overview.md file: -->
  <description overview="README.md">Board Support Pack for M251/M252/M254/M256/M258 series</description>
  <!-- Optional - pack download URL (if publicly available): -->
  <url>https://github.com/OpenNuvoton/keil_packs/raw/master/m251_bsp/</url>
  <!-- Optional - Git repo URL (if applicable): -->
  <repository type="git">https://github.com/OpenNuvoton/M251BSP.git</repository>
  <!-- Strongly recommended - support contact information: -->
  <supportContact></supportContact>
   <!-- Must have - licensing information (for software BOM): -->
  <license>LICENSE</license>
  <licenseSets>
    <licenseSet id="all" default="true" gating="true">
      <license name="LICENSE" title="Apache 2.0 License"/>
    </licenseSet>
  </licenseSets>
 
  <!-- Must have - pack release information for versioning: -->
  <releases>
    <release version="3.02.008" date="2025-10-08">
      - Updated standard driver: I2C, PDMA, SPI.
      - New sample code: EADC_AverageCMP, QSPI_PDMA_Master, QSPI_PDMA_Slave.
      - All VS Code projects upgraded to use GCC 14.3.1.
      - Updated sample code: BPWM_DoubleBuffer, ISP_I2C, ISP_SPI, PWM_DeadTime, PWM_DoubleBuffer, TIMER_PWM_ChangeDuty, USBD_HID_RemoteWakeup.
      - Updated tool: TouchView and TouchView_MP replaced NuSenadj and NuSenadj_MP.
    </release>
  </releases>

  <keywords>
    <!-- Optional - keywords for indexing: -->
    <keyword>Nuvoton</keyword>
    <keyword>Board Support Pack</keyword>
    <keyword>NuMicro_M251</keyword>
    <keyword>M251/M252/M254/M256/M258 BSP</keyword>
    <keyword>M252KG6AE</keyword>
    <keyword>M256SD2AE</keyword>
    <keyword>M258KE3AE</keyword>
    <keyword>M258KG6AE</keyword>
    <keyword>NuMaker-M252KG</keyword>
    <keyword>NuMaker-M256SD</keyword>
    <keyword>NuMaker-M258KE</keyword>
    <keyword>NuMaker-M258KG</keyword>
  </keywords>

  <requirements>
    <!-- Strongly recommended - requirement to DFP (or other packs): -->
    <packages>
      <package vendor="Nuvoton" name="NuMicroM23_DFP" version="1.0.1"/>
    </packages>
  </requirements>

  <!-- Must have - boards section: -->
  <boards>
    <!-- Must have - board information: -->
    <board vendor="Nuvoton" name="NuMaker-M252KG" revision="V1.0" salesContact="sales@nuvoton.com" orderForm="https://direct.nuvoton.com/en/numaker-m252kg">
      <!-- Strongly recommended - board description: -->
      <description>NuMaker-M252KG Board</description>
      <!-- Must have - on-board device information: -->
      <mountedDevice    deviceIndex="0" Dvendor="Nuvoton:18" Dname="M252KG6AE"/>
      <!-- Strongly recommended - on-board device compatibility information: -->
      <compatibleDevice deviceIndex="0" Dvendor="Nuvoton:18" Dname="M251*"/>
      <compatibleDevice deviceIndex="0" Dvendor="Nuvoton:18" Dname="M252*"/>
      <compatibleDevice deviceIndex="0" Dvendor="Nuvoton:18" Dname="M254*"/>
      <compatibleDevice deviceIndex="0" Dvendor="Nuvoton:18" Dname="M256*"/>
      <compatibleDevice deviceIndex="0" Dvendor="Nuvoton:18" Dname="M258*"/>
      <!-- Strongly recommended - board features: -->
      <feature type="ODbg"      n="1"                name="On-board Nu-Link2-Me"/>
      <feature type="PWR"       n="5"                name="USB Powered"/>
      <feature type="PWR"       n="3.3" m="5"        name="POWER Connector for external supply"/>
      <feature type="DIO"       n="128"              name="4 extended connectors to all chip pins"/>
      <feature type="LED"       n="1"                name="User LED"/>
      <feature type="Button"    n="1"                name="User Button"/>
      <feature type="USB"       n="1"                name="USB FS Connector"/>
      <!-- Strongly recommended - on-board debug probe information (if applicable): -->
      <debugInterface adapter="Nu-Link" connector="Mini-USB"/>
    </board>
  </boards>
  
